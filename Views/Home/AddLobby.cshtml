@{
    ViewData["Title"] = "add lobby";
}

<div class="">
    <h1>@ViewData["Title"]</h1>

    <p>add a lobby, this is the get page</p>

    <div class="row">
        <div class="col-md-4">
            <form asp-action="AddLobby" enctype="multipart/form-data">
                <div class="form-group">
                    <label class="control-label"> id </label>
                    <input name="lobbyId" class="form-control" type="number" placeholder="11223344" min="0" required />
                    <label class="control-label"> best of</label>
                    <input name="bestOf" class="form-control" type="number" placeholder="0" min="0" />
                    <label class="control-label"> warmup count </label>
                    <input name="warmupCount" class="form-control" tySpe="number" placeholder="0" min="0" />
                    <hr>
                    <input class="btn btn-primary" type="submit" value="add" />
                </div>
            </form>
        </div>
    </div>
</div>
